#cbl{
    width: 6rem;
    height: 6rem;
    position:fixed;
    bottom: 50%;
	top:50%;
    left: 92%;
    box-sizing: border-box;
    opacity: 0.5;
    z-index: 1000;
    border-radius: 50%;
    overflow: hidden;
	transition:opacity 0.5s linear;
	cursor:pointer;
}
#cbl:hover{
    opacity:0.8;
}
#cbl>input{
    position:absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    display: none;
    
}
